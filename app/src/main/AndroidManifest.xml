<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kirei.nestapp2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.NestApp2">
        <activity android:name="com.kirei.nestapp2.activity.SignUpActivity"></activity>
        <activity android:name="com.kirei.nestapp2.activity.SignInActivity" />
        <activity android:name="com.kirei.nestapp2.activity.ProfileActivity" />
        <activity android:name=".ForgotPassword" />
        <activity android:name="com.kirei.nestapp2.activity.DetailActivity" />
        <activity android:name="com.kirei.nestapp2.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>